{
  "name": "cumo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"node scripts/build-electron.mjs --watch\" \"wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "vite build",
    "build:electron": "node scripts/build-electron.mjs",
    "build:icons": "node scripts/build-icons.mjs",
    "build:backend": "node scripts/build-backend.mjs",
    "dist": "npm run build:renderer && npm run build:electron && npm run build:icons && npm run build:backend && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:build": "node scripts/build-electron.mjs",
    "electron:dev": "node scripts/build-electron.mjs --watch",
    "start": "electron .",
    "test": "vitest"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.18.6",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "icon-gen": "^3.0.1",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.3",
    "vite": "^7.2.4",
    "vitest": "^2.1.9",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.cumo.app",
    "productName": "Cumo",
    "asar": true,
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "dist/**",
      "dist-electron/**",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "backend/dist",
        "to": "backend",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "backend/credentials.json",
        "to": "backend/credentials.json"
      }
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/icons"
    }
  }
}
